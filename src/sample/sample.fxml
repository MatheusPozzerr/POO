<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label minWidth="-Infinity" text="Bem Vindo ao ACMEJobs" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.rowIndex="1">
               <font>
                  <Font name="System Bold" size="24.0" />
               </font></Label>
            <Label minWidth="-Infinity" text="Selecione a sua opção:" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <MenuButton fx:id="Selecionar" minWidth="-Infinity" mnemonicParsing="false" text="Selecionar" GridPane.columnIndex="4" GridPane.rowIndex="2">
              <items>
                <MenuItem fx:id="BotaoAtendente" mnemonicParsing="false" onAction="#SelecionaAtendente" text="Atendente" />
                <MenuItem fx:id="BotaoPrestador" mnemonicParsing="false" onAction="#SelecionaPrestador" text="Prestador" />
                  <MenuItem fx:id="BotaoAdministracao" mnemonicParsing="false" onAction="#SelecionaAdminsitracao" text="Administrador" />
              </items>
            </MenuButton>
            <Button fx:id="BotaoConfirmar" minWidth="-Infinity" mnemonicParsing="false" onAction="#ClicaConfirmar" text="Confirmar" GridPane.columnIndex="4" GridPane.halignment="LEFT" GridPane.rowIndex="6" GridPane.valignment="BOTTOM">
               <GridPane.margin>
                  <Insets left="30.0" />
               </GridPane.margin></Button>
            <Button fx:id="BotaoSair" mnemonicParsing="false" onAction="#ClicaSair" text="Sair" GridPane.columnIndex="6" GridPane.halignment="LEFT" GridPane.rowIndex="6" GridPane.valignment="BOTTOM" />
            <TextField fx:id="TextCPF" minWidth="-Infinity" onAction="#ClicaConfirmar" visible="false" GridPane.columnIndex="3" GridPane.rowIndex="4" />
            <Label fx:id="LabelCpf" minWidth="-Infinity" text="CPF Prestador:" visible="false" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Label fx:id="LabelAvisos" minWidth="-Infinity" GridPane.columnIndex="1" GridPane.rowIndex="5" />
         </children>
      </GridPane>
   </children>
</GridPane>
